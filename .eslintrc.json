{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-unused-vars":"off",
        "no-param-reassign":"off",
        "no-unreachable": "off",
        "class-methods-use-this":"off",
        "linebreak-style": "off",
        "camelcase": "off",
        "no-restricted-syntax": "off",
        "no-await-in-loop": "off",
        "no-undef": "off"
    }
}